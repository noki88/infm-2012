class TwoPlayerStrategicGame(object):

    def __init__(self, game_title, player_names, strategies, payoffs, outcomes):
        self.game_title = game_title
        self.player_names = player_names
        self.strategies = strategies
        self.payoffs = payoffs
        self.outcomes = outcomes
        
    def __str__(self):
        s = ""
        s += "Game Title:", self.game_title
        s += " Player Names:", self.player_names 
        s += " Strategies:", self.strategies
        s += " Payoffs:", self.payoffs
        s += " Outcomes:", self.outcomes
        
    
        